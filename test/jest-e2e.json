{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/repositories/(.*)": "<rootDir>/../libs/repositories/src/$1",
    "@app/repositories": "<rootDir>/../libs/repositories/src",
    "@app/interfaces/(.*)": "<rootDir>/../libs/interfaces/src/$1",
    "@app/interfaces": "<rootDir>/../libs/interfaces/src",
    "@app/services/(.*)": "<rootDir>/../libs/services/src/$1",
    "@app/services": "<rootDir>/../libs/services/src",
    "@app/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@app/entities": "<rootDir>/../libs/entities/src",
    "@app/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@app/logger": "<rootDir>/../libs/logger/src",
    "@app/modules/(.*)": "<rootDir>/../libs/modules/src/$1",
    "@app/modules": "<rootDir>/../libs/modules/src",
    "@app/configs/(.*)": "<rootDir>/../libs/configs/src/$1",
    "@app/configs": "<rootDir>/../libs/configs/src",
    "@app/constants/(.*)": "<rootDir>/../libs/constants/src/$1",
    "@app/constants": "<rootDir>/../libs/constants/src",
    "@app/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@app/common": "<rootDir>/../libs/common/src"
  }
}